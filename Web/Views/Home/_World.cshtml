@model Web.Models.CreatorViewModel

<table id="tableAgents">
    @for (int i = 0; i < Model.Creator.World.Matrix.GetLength(0); i++)
    {
        <tr class="row">
            @for (int j = 0; j < Model.Creator.World.Matrix.GetLength(1); j++)
            {
                <td class="">
                    @{
                        var item = Model.Creator.World.Matrix[i, j];
                    }
                    <img src="@item.getIcon()" class="icon" alt="@item.ToString()" />
                </td>
            }
        </tr>
    }
</table>